@{
    ViewData["Title"] = "Lista Usuarios Postulantes";
}

<div class="btn-group btn-lg btn-block" role="group" aria-label="Basic example">
    <button type="button" class="btn btn-outline-primary " onclick="location='/Administrador/Perfil'">Mi Perfil</button>
    <button type="button" class="btn btn-outline-primary active">Usuarios</button>
    <button type="button" class="btn btn-outline-primary" onclick="location='/Administrador/Ofertas_Publicadas'">Ofertas Publicadas</button>
    <button type="button" class="btn btn-outline-primary" onclick="location='/Administrador/Postulaciones'">Postulaciones</button>
    <button type="button" class="btn btn-outline-primary" onclick="location='/Administrador/Reportes'">Reportes</button>
    <button type="button" class="btn btn-outline-primary" onclick="location='/Administrador/Sugerencias'">Sugerencias</button>
</div>

<br>
<br>

<div class="text-left">
    <div class="btn-group btn-group-toggle" role="group" aria-label="Basic example">
        <button type="button" class="btn btn-secondary" onclick="location='/Administrador/Usuarios_Empresas'">Empresas</button>
        <button type="button" class="btn btn-secondary active">Postulantes</button>
    </div>
</div>

<br>

<table id="tbListadoPostulante" class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Nombres</th>
            <th scope="col">Apellidos</th>
            <th scope="col">DNI</th>
            <th scope="col">Fecha Nac.</th>
            <th scope="col">Correo</th>
            <th scope="col">Telefono</th>
        </tr>
    </thead>

</table>
