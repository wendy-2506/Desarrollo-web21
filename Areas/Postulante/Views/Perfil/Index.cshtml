<h1>ESTA ES AREA DE POSTULANTE PERFIL</h1>

<div class="btn-group btn-lg btn-block" role="group" aria-label="Basic example">
    <button type="button" class="btn btn-outline-primary" onclick="location='/Postulante/Publicaciones'">Publicaciones</button>
    <button type="button" class="btn btn-outline-primary" onclick="location='/Postulante/MisOfertas'">Mis Postulaciones</button>
    <button type="button" class="btn btn-outline-primary active">Mi perfil</button>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Editar Perfil </button>
    <button type="button" class="btn btn-primary" id="btnGuardarCliente">Desactivar Cuenta</button>
</div>

<form>
    <div class="form-row">
        <div class="file-field col-md-4">
            <img src="https://mdbootstrap.com/img/Photos/Others/placeholder.jpg" class="img-fluid"
                 alt="example placeholder">
        </div>

        <div class="form-group col-md-6">
            <label for="inputNombre">NOMBRE</label>
            <input type="text" class="form-control" id="inputNombre">
            <label for="inputApellido">Apellido</label>
            <input type="text" class="form-control" id="inputApellido">
            <label for="inputDNI">DNI</label>
            <input type="text" class="form-control" id="inputDNI">
            <label for="inputFechNac">Fecha De Nacimiento</label>
            <input type="text" class="form-control" id="inputFechNac">
            <label for="inputEmail">Correo</label>
            <input type="text" class="form-control" id="inputEmail">
            
        </div>

    </div>

    
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="inputGroupFileAddon01">CARGAR CV</span>
        </div>
        <div class="custom-file">
            <input type="file" class="custom-file-input" id="inputGroupFile01" aria-describedby="inputGroupFileAddon01">
            <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
        </div>
    </div>

    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">DESCRIPCION</span>
        </div>
        <textarea class="form-control" aria-label="With textarea"></textarea>
    </div>
</form>















