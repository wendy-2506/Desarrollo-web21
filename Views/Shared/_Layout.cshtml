<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    <title>@ViewData["Title"] - Tu_Nuevo_Trabajo2021</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body style="background-color: bisque;">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3" >
            <div class="container-fluid" id="encabezado">
                <a class="navbar-brand" href="#">
                    <img src="~/imagenes/logoweb.jpg" width="380" height="80">
                </a>
                @*<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Tu_Nuevo_Trabajo2021</a>*@
                <ul class="nav justify-content-end">
                    <li class="nav-item">
                        <button href="../Views/Loginn/Login.cshtml" type="button" class="btn btn-link">Registrarse</button>
                    </li>
                    <li class="nav-item">
                        <input type="button" class="btn btn-primary" value="Ingresar" onclick="location='/LOGIN2/Home/Login'" />@*cambie esto para ridereccionar*@
                    </li>
                </ul>

            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Tu Nuevo Trabajo 2021 - <a asp-area="" asp-controller="Home" asp-action="Grupo">Grupo 10mo ciclo</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
